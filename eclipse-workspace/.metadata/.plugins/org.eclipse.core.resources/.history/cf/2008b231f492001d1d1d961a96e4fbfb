package chapter08;

import chapter08.Apple.Color;

public class UtilsExample {
	public static void main(String[] args) {
		Apple[] apples = {
				new Apple("1번",Color.GREEN),
				new Apple("2번",Color.RED),
				new Apple("3번",Color.YELLOW),
				new Apple("4번",Color.GREEN),
				new Apple("5번",Color.RED),
				new Apple("6번",Color.GREEN),
				new Apple("7번",Color.RED),
				new Apple("8번",Color.YELLOW),
				new Apple("9번",Color.GREEN),
				new Apple("10번",Color.RED),
		};
		Utils utils = new Utils();
		Apple[] apples2 = utils.filter(apples, Color.RED);
		for(Apple apple : apples2) {
			System.out.println(apple);
		}
	}
}
